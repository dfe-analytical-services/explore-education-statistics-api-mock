{
  "name": "ees-public-api-mock",
  "version": "1.0.0",
  "description": "",
  "license": "UNLICENSED",
  "private": true,
  "main": "dist/functions/*.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && swc src -d dist --copy-files",
    "build:watch": "npm run clean && swc src -d dist --copy-files --watch",
    "dev:func": "concurrently --raw npm:build:watch npm:start:func",
    "dev:server": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "import": "ts-node --transpile-only src/scripts/import.ts",
    "start:func": "func start",
    "start:server": "node dist/server.js",
    "schema": "openapi --input src/openapi.yaml --output src/schema --useUnionTypes --exportServices false --exportCore false",
    "tsc": "tsc --noEmit"
  },
  "type": "commonjs",
  "dependencies": {
    "@azure/functions": "^4.0.1",
    "archiver": "^5.3.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "duckdb": "^0.9.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-openapi-validator": "^5.0.6",
    "hashids": "^2.3.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "papaparse": "^5.4.1",
    "qs": "^6.11.2",
    "slugify": "^1.6.6",
    "sql-formatter": "^13.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.91",
    "@types/archiver": "^5.3.3",
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.18",
    "@types/fs-extra": "^11.0.2",
    "@types/lodash": "^4.14.199",
    "@types/morgan": "^1.9.6",
    "@types/node": "^18.18.3",
    "@types/papaparse": "^5.3.9",
    "@types/qs": "^6.9.8",
    "azure-functions-core-tools": "^4.x",
    "concurrently": "^8.2.1",
    "fs-extra": "^11.1.1",
    "openapi-typescript-codegen": "^0.25.0",
    "prettier": "^3.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.3.3",
    "typescript": "^5.2.2"
  }
}
